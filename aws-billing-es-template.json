{
  "template" : "aws-billing-*",
  "settings" : {
    "index.refresh_interval" : "10s"
  },
  "mappings" : {
    "_default_" : {
       "_all" : {"enabled" : true},
       "dynamic_templates" : [ {
         "string_fields" : {
           "match" : "*",
           "match_mapping_type" : "string",
           "mapping" : {
             "type" : "text",
	     "index" : true,
	     "norms" : true,
             "fields" : {
                 "keyword" : {"type": "keyword"
             }
           }
         }
       } ],
       "properties" : {
         "@version": { "type": "keyword"},
	 "lineItem/BlendedCost":{"type":"float"},
	 "lineItem/UnblendedCost":{"type":"float"}
       }
    }
  }
}
